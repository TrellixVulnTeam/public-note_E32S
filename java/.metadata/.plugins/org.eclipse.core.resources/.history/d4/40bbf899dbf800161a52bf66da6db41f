package demo;

import java.util.HashMap;
import java.util.Map;

public class Main {

	public static void main(String[] args) {
		Map<Fruit, String> properties = new HashMap<Fruit, String>(3);
		new Fruit(0).hashCode();
		properties.put(new Fruit(0), "efg");
		properties.put(new Fruit(1), "efg");
		properties.put(new Fruit(2), "efg");
		properties.put(new Fruit(3), "efg");
	}

}

class Fruit{
	int val = 0;
	public Fruit(int val){
		this.val = val;
	}
	
	@Override
	public int hashCode() {
		return 0;
	}
}