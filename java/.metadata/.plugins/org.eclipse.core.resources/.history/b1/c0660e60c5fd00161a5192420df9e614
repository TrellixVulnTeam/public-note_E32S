package com.lsj.util;

import java.util.Arrays;

public class LPriorityQueue<T extends Comparable<? super T>> implements LQueue<T>{

	private T[] items;
	private int size;
	private int tail;
	
	@Override
	public int size() {
		return size;
	}

	@Override
	public boolean isEmpty() {
		return size == 0;
	}

	@Override
	public boolean add(T element) {
		ensuerCapacity(size+1);
		items[size+1];
		return false;
	}

	@Override
	public T poll() {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public T remove() {
		// TODO Auto-generated method stub
		return null;
	}
	
	private void ensuerCapacity(int nextSize){
		if(nextSize == items.length){
			items = Arrays.copyOf(items, items.length<<1);
		}
	}

}
