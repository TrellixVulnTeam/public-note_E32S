package demo;

import java.util.ArrayList;
import java.util.List;

public class Main {

	public static void main(String[] args) {
		List<Integer> list = new ArrayList<>();
		list.add(1);
		list.add(2);
		list.add(3);
		list.add(4);
		list.add(5);
	
		ArrayList<Integer> l2 = (ArrayList<Integer>) ((ArrayList<Integer>)list).clone();
		l2.set(0, 3);
		for(Integer num : l2){
			System.out.println(num);
		}
		
		for(Integer num : l2){
			System.out.println(num);
		}
	}

}
