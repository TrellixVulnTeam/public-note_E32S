package demo;

public class Main {

	public static void main(String[] args) {
		Store<Employee> o = new Store<>();
		o.set(new Person());
	}
}

class Store<T>{
	private T obj;
	public void set(T obj){
		this.obj = obj;
	}
	
	public T get(){
		return obj;
	}
}

class Person{}

class Employee extends Person{}
class Singer extends Person{}