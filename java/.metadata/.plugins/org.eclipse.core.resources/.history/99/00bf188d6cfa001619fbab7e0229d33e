package demo;

public class Main {

	public static void main(String[] args) {
		
		Store<? extends Person> o1 = new Store<Employee>();
		Store<? super Employee> o2 = new Store<Person>();
		
		
		o1.set(new Employee());
		o2.set(new Employee());
	}
}

class Store<T>{
	private T obj;
	public void set(T obj){
		this.obj = obj;
	}
	
	public T get(){
		return obj;
	}
}

class Person{}

class Employee extends Person{}
class Singer extends Person{}