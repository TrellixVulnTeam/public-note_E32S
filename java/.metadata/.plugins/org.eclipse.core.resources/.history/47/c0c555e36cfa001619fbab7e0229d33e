package demo

public class Main {

	public static void main(String[] args) {
		
		//存放父类，方法对象可以是子类
		Store<? super Employee> o1 = new Store<Person>();
		o1.set(new Employee());
		
		Store<? extends Person> o2 = new Store<Employee>();
		o2.set(new Employee());
		
	}
}

class Store<T>{
	private T obj;
	public void set(T obj){
		this.obj = obj;
	}
	
	public T get(){
		return obj;
	}
}

class Person{}

class Employee extends Person{}
class Singer extends Person{}